{% extends 'base.html.twig' %}

{% block title %}Coachs index{% endblock %}

{% block body %}
    <h1>Coachs index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Prenom</th>
                <th>Specialite</th>
                <th>Prix</th>
                <th>Photo</th>
                <th>Description</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for coach in coachs %}
            <tr>
                <td>{{ coach.id }}</td>
                <td>{{ coach.nom }}</td>
                <td>{{ coach.prenom }}</td>
                <td>{{ coach.specialite }}</td>
                <td>{{ coach.prix }}</td>
                <td>{{ coach.photo }}</td>
                <td>{{ coach.description }}</td>
                <td>
                    <a href="{{ path('coachs_show', {'id': coach.id}) }}">show</a>
                    <a href="{{ path('coachs_edit', {'id': coach.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="8">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('coachs_new') }}">Create new</a>
{% endblock %}
